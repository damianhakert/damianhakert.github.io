.wrapper
  .press-content
    %h1 PRESS AND LOGOS
    %h3
      For press enquiries, please email:
      %span
        press@gitlab.com
        %span
  .sub-wrapper
    .container.text-center
      .row
        .col-md-12.recent-news
          %h2 RECENT NEWS
          .recent-blog
            %p
              For press releases, please
              %a{href: "https://about.gitlab.com/press/releases/"} visit our Press Releases page.
            %p
              For updates from the GitLab team, please
              %a{href: "https://about.gitlab.com/blog/archives.html"} visit our blog
              or
              = succeed "." do
                %a{href: "https://twitter.com/gitlab/"} view our Twitter feed
      %hr.divider/
      .row
        .col-md-12.recent-podcasts
          %h2 PODCASTS
          .podcasts
            .podcast-item
              .podcast-date
                SEPTEMBER 2016
              .press-text
                %a{href: "https://changelog.com/podcast/220"}
                  .article-title
                    Changelog: GitLab Master Plan
            .podcast-item
              .podcast-date
                OCTOBER 2014
              .press-text
                %a{href: "http://blog.bitnami.com/2014/10/bitnami-open-source-leaders-interview.html"}
                  .article-title
                    Bitnami Open Source Leaders Interview Series
            .podcast-item
              .podcast-date
                AUGUST 2014
              .press-text
                %a{href: "http://www.binpress.com/blog/2014/08/05/binpress-podcast-episode-5-sytse-sijbrandij-gitlab/"}
                  .article-title
                    Binpress podcast
            .podcast-item
              .podcast-date
                NOVEMBER 2013
              .press-text
                %a{href: "http://episodes.gitminutes.com/2013/11/gitminutes-25-sytse-sijbrandij-from.html"}
                  .article-title
                    Git Minutes (Nov 2013)
            .podcast-item
              .podcast-date
                SEPTEMBER 2013
              .press-text
                %a{href: "http://thechangelog.com/103/"}
                  .article-title
